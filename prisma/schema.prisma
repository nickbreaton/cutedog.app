generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../data/database.sqlite"
}

model Pet {
  id       Int    @id @unique() @default(autoincrement())
  username String @unique()
  name     String
  owners   User[]
}

model User {
  id       Int    @id @unique() @default(autoincrement())
  email    String @unique()
  pets     Pet[]
}
